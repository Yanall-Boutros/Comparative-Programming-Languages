@% $Id$

remainder( Float, Modulus, Quotient, Remainder) :-
   Quotient is truncate( Float / Modulus),
   Remainder is Float - Quotient * Modulus.

remainder( 93.468, 60, Q, R).

remainder( 18.62849, 60, Q, R).

@
