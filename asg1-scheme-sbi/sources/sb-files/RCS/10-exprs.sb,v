a8 2
	let pi = 4 * atan(1)
	let e = exp(1)
d10 23
a32 23
	print "1+1      = ", 1+1
	print "2-2      = ", 2- 2
	print "3*3      = ", 3*3
	print "4/9      = ", 4/9
	print "2^10     = ", 2^10
