1.2
log
@-
@
text
@# $Id: 22-fibonacci.sb,v 1.1 2019-01-18 11:47:25-08 - - $
#
# Print out all Fibonacci numbers up to max.
#
        let max = 10^6

        let fib0 = 0
        let fib1 = 1
        print "fib(", 0, ")=", fib0
        print "fib(", 1, ")=", fib1
        let i=1
loop:   let fib = fib0 + fib1
        let i=i+1
        print "fib(", i, ")=", fib
        let fib0 = fib1
        let fib1 = fib
        if fib <= max goto loop
@


1.1
log
@-
@
text
@d1 1
a1 1
# $Id: 22-fibonacci.sb,v 1.1 2008-02-05 15:34:36-08 - - $
d5 1
a5 1
	let max = 10^6
d7 11
a17 11
	let fib0 = 0
	let fib1 = 1
	print "fib(", 0, ")=", fib0
	print "fib(", 1, ")=", fib1
	let i=1
loop:	let fib = fib0 + fib1
	let i=i+1
	print "fib(", i, ")=", fib
	let fib0 = fib1
	let fib1 = fib
	if fib <= max goto loop
@
