@#!/bin/sh
# $Id: xcd,v 1.5 2018-08-22 19:04:09-07 - - $

for dir in $*
do
   shellcmd="cd $dir; xtlabel $dir; exec /bin/bash --login"
   echo `basename $0`: $shellcmd
   xterm -ut +ls -e sh -c "$shellcmd" &
   sleep 1
done

@
