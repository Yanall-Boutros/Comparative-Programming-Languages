@#!/bin/sh
# $Id: mkrecheaders,v 1.8 2019-04-03 16:36:05-07 - - $

if [ $# -eq 0 ]
then
   set . $*
fi

for dir in $*
do
   for subdir in $(find $dir -type d -name RCS -prune \
                          -o -type d -name '.?*' -prune \
                          -o -type d -print)
   do
      (cd $subdir; mkheader)
   done
done
@
